
    @using ITS.Infrastructure.Global;
    @using (Html.BeginForm(GlobalConst.Actions.CaseSearchController.BookInitialAppointment, GlobalConst.Controllers.CaseSearch, FormMethod.Post, new
{
    @class = "form-horizontal",
    id = "frmUpdateIABookedDate",
    enctype = "multipart/form-data",
    data_bind = "ajaxForm:{ success: function(responseText, statusText, xhr, $form){ updateIAFormSuccess(responseText, statusText, xhr, $form); },beforeSubmit: InitialFromBeforeSubmit }"
}))
    {
        <table class="table table-bordered table-striped table-hover sortable">
            <thead>
                <tr>
                    <th>Appointment Date
                    </th>
                    <th>Appointment Time
                    </th>
                     <th>First Appointment Offered Date
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        <div class="control-group">
                            <div class="controls" style="margin-left: 0px"> 
                                <div class="text-center">
                                    <input type="text" id="CaseAppointmentDateAppointmentDate" data-bind="enable: EnableIA(), value:CaseAppointmentDateAppointmentDate,jqBootstrapValidation: {}" maxlength="10"
                                        data-validation-regex-message="DD/MM/YYYY" data-validation-regex-regex="^(0[1-9]|[12][0-9]|3[01])[- /.](0[1-9]|1[012])[- /.](19|20)\d\d+$" required />
                                </div>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="control-group">
                            <div class="text-center">
                                <input type="text" id="CaseAppointmentDateAppointmentTime" data-bind="enable: EnableIA(), value:CaseAppointmentDateAppointmentTime,jqBootstrapValidation: {}" maxlength="7"
                                    data-validation-regex-regex="^([0-1][0-9]|2[0-3]):([0-5][0-9])$" required />
                                <input type="hidden" name="AppointmentDateTime" data-bind="value:AppointmentDateTime()" />
                                <input type="hidden" name="CaseID" data-bind="value:CaseID" />
                                <input type="hidden" name="CaseIDPK" data-bind="value:CaseIDPK" />
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="control-group">
                            <div class="controls" style="margin-left: 0px">
                                <div class="text-center">
                                    <input type="text" id="FirstAppointmentOfferedDate" data-bind="enable: EnableIA(), value:FirstAppointmentOfferedDate,jqBootstrapValidation: {}" maxlength="10"
                                        data-validation-regex-message="DD/MM/YYYY" data-validation-regex-regex="^(0[1-9]|[12][0-9]|3[01])[- /.](0[1-9]|1[012])[- /.](19|20)\d\d+$" required />
                                    <input type="hidden" name="FirstAppointmentOfferedDate" data-bind="value:FirstAppointmentOfferedDate()" />
                                </div>
                            </div>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
        <div class="form-actions">
            <div class="column-groups-button">
                <button class="btn btn-primary btn-block" type="submit" id="btnSaveIA" data-bind="enable: EnableIA()">Update</button>

            </div>
        </div>    
    }
    <div class="widget-title">
        <span class="icon"><i class="icon-th"></i></span>
        <h5>Unsuccessfull Call</h5>
    </div>
    <table class="table table-bordered table-striped table-hover">
        <thead>
            <tr>
                <th style="text-align:left;">Unsuccessfull Call Date
                </th>
            </tr>
        </thead>
        <tbody data-bind="foreach:CaseUnsuccessfullContactDates">
            <tr>
              <td  data-bind="text: moment(ContactAttemptDate).format('DD/MM/YYYY')"></td>
            </tr>
        </tbody>
        <tbody>
        </tbody>
        <tfoot>
             <!--ko if:CaseUnsuccessfullContactDates().length == 0 -->
            <tr>
                <td colspan="4">
                    <div class="alert alert-info">
                        <button type="button" class="close" data-dismiss="alert">
                            &times;</button>
                        No records are found.
                    </div>
                </td>
            </tr>
             <!-- /ko -->
        </tfoot>
    </table>
  <!--ko if:FirstAppointmentOfferedDate() != null -->
    <div class="widget-title">
        <span class="icon"><i class="icon-th"></i></span>
        <h5>Patient Contact</h5>
    </div>
    <table class="table table-bordered table-striped table-hover">
        <thead>
            <tr>
                <th style="text-align:left;">Date of Contact 
                </th>
            </tr>
        </thead>
            <tr>               
                <td data-bind="text: FirstAppointmentOfferedDate"></td>
            </tr>
        <tbody>
        </tbody>
    </table>
     <!-- /ko -->


 