
@*@model ITS.Domain.Models.SupplierSearch*@
<table class="table table-bordered table-striped table-hover sortable">
    <thead>
        <tr>
            <th>
                Action
            </th>
            <th>
                Supplier Name
            </th>
            <th>
                Treatment Category
            </th>
            <th>
                Post Code
            </th>
            
        </tr>
    </thead>
    <tbody data-bind="foreach:Suppliers">
        <tr>
            <td>
                <a data-bind="attr: { href: '/Supplier/Detail/' + SupplierID() }">
                          View </a>
            </td>
            <td data-bind="text:SupplierName">
            </td>
            <td data-bind="text:TreatmentCategoryName">
            </td>
            <td data-bind="text:PostCode">
            </td>
           
        </tr>
    </tbody>
</table>



